{
  "name": "express-snipets",
  "displayName": "NodeJs code Snippet",
  "description": "Handy Node.js and Express.js code snippets for VS Code.",
  "url": "https://github.com/tushar-kant/nodejs-snippet",
  "version": "1.2.3",
  "publisher": "Zyronix",
  "icon": "node.jpeg",
  "engines": {
    "vscode": "^1.92.0"
  },
  "categories": [
    "Snippets",
    "Programming Languages",
    "Formatters"
  ],
  "contributes": {
    "snippets": [
      {
        "language": "node.js",
        "path": "./snippets/snippets.code-snippets"
      },
      {
        "language": "javascript",
        "path": "./snippets/node.snippets.json"
      },
      {
        "language": "json",
        "path": "./snippets/express.snippets.json"
      }
    ]
  }
}